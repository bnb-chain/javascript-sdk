{
  "name": "@binance-chain/javascript-sdk",
  "version": "2.2.0",
  "main": "lib/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "test:ledger": "qunit --require babel-polyfill tests-ledger/tests.js",
    "test:ledger:browser": "./tests-ledger/run-browser-tests.sh",
    "test:ledger:jest": "yarn test --runTestsByPath --verbose __tests__/ledger-node.test.js",
    "build": "rm -rf lib && npx babel src -d lib",
    "prepublishOnly": "npm run build",
    "lint": "eslint --fix src __tests__"
  },
  "dependencies": {
    "@ledgerhq/hw-transport-node-hid": "^4.33.3",
    "@ledgerhq/hw-transport-u2f": "^4.32.0",
    "@ledgerhq/hw-transport-web-ble": "^4.32.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "base32-encode": "^1.1.0",
    "bech32": "^1.1.3",
    "bip32": "^1.0.2",
    "bip39": "^2.5.0",
    "bn.js": "^4.11.8",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^3.1.9-1",
    "lodash": "^4.17.11",
    "safe-buffer": "^5.1.2",
    "secp256k1": "^3.5.2",
    "secure-random": "^1.1.1",
    "tiny-secp256k1": "^1.0.1",
    "uuid": "^3.3.2",
    "varint": "^5.0.0",
    "varstruct": "^6.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "browserify": "^16.2.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.11.1",
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "qunit": "^2.9.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-async-to-generator"
    ]
  },
  "files": [
    "lib/*"
  ]
